server {
    listen 80;
    server_name _;

    location /redirect_me {
        return 301 https://www.youtube.com/watch?v=QH2-TGUlwu4;
    }

    location = /error_404.html {
        internal;
        root /var/www/html;
    }

    error_page 404 /error_404.html;

    location /error_404.html {
        internal;
        default_type "text/html";
        return 404 "Ceci n'est pas une page";
    }
}
